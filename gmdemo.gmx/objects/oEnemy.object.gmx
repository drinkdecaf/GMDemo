IncomingBullet = instance_place(x,y,oBullet)

if instance_exists(IncomingBullet) {
    
    //Destroy the bullet
    with IncomingBullet {
        instance_destroy()
    }

    //Shrink ourselves
    image_xscale = image_xscale - 0.2
    image_yscale = image_xscale
    
    if speed &lt; 11 {
        speed += 1
